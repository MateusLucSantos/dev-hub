{
  "expo": {
    "name": "dev-hub",
    "slug": "dev-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mateusbsantos.devhub"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mateusbsantos.devhub"
    },
    "web": {
      "favicon": ""
    },
    "plugins": [
      "expo-font",
      "react-native-edge-to-edge"
    ]
  }
}
